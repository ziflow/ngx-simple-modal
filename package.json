{
  "name": "@ziflow/ngx-simple-modal",
  "version": "16.0.0-dev.1",
  "description": "A simple unopinionated framework to implement simple modal based behaviour in angular (v2+) projects.",
  "scripts": {
    "demo-ng5": "cd demo-ng5 && npm i && ng serve",
    "demo-ng6": "cd demo-ng6 && npm i && ng serve",
    "demo-ng8": "cd demo-ng8 && npm i && ng serve",
    "demo-ng16": "cd demo-ng16 && npm i && ng serve",
    "lint": "./node_modules/.bin/tslint src/**/*.ts",
    "prepublish": "npm run build",
    "build": "ng-packagr --config src/tsconfig.build.json",

    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "release": "npm run build && cd dist && release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevcjones/ngx-simple-modal.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "angular4",
    "angular5",
    "angular6",
    "angular7",
    "angular8",
    "angular9",
    "angularx",
    "extendable",
    "ngx",
    "simple",
    "smart",
    "dialog",
    "modal",
    "typescript",
    "component",
    "ui"
  ],
  "author": "Team @ Ziflow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ziflow/ngx-simple-modal/issues"
  },
  "homepage": "https://github.com/ziflow/ngx-simple-modal#readme",
  "dependencies": {},
  "devDependencies": {
    "@angular/animations": "^16.0.3",
    "@angular/cli": "^16.0.2",
    "@angular/common": "^16.0.3",
    "@angular/compiler": "^16.0.3",
    "@angular/compiler-cli": "^16.0.3",
    "@angular/core": "^16.0.3",
    "@angular/platform-browser": "^16.0.3",
    "@angular/platform-browser-dynamic": "^16.0.3",
    "@angular/platform-server": "^16.0.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^6.0.102",
    "jest": "^29.5.0",
    "jest-preset-angular": "^13.1.1",
    "ng-packagr": "^16.0.1",
    "rxjs": "^6.0.0",
    "typescript": "^5.0.4",
    "ts-jest": "^29.1.0",
    "zone.js": "^0.13.0"
  }
}
